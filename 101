(fn [s t]
    (let [z count ! nth
          - dec
          d (memoize (fn [d i j]
                       (if (= i 0) j
                         (if (= j 0) i
                           (let [a (- i) b (- j) c (d d a b)]
                             (if (= (! s a) (! t b))
                               c
                               (+ 1 (min c
                                         (d d i b)
                                         (d d a j)))))))))
    ]
  (d d (z s) (z t))))

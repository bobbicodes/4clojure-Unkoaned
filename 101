(memoize 
  (fn ml [p q]
    (let [lp (count p) lq (count q)]
    (if (or (= 0 lp) (= 0 lq))
      (max lp lq)
      (let [rp (rest p) rq (rest q)]
        (if (= (first p) (first q))
          (ml rp rq)
          (inc (min (ml rp q) (ml p rq) (ml rp rq)))))))))

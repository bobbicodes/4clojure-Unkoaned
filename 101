(fn levenshtein-distance [w1 w2]
  (let [f (fn [acc [i j]]
            (conj acc 
                  [[i j] 
                   (if (zero? (min i j)) 
                     (max i j)
                     (min (inc (acc [(dec i) j]))
                          (inc (acc [i (dec j)]))
                          (+ (acc [(dec i) (dec j)])
                             (if (= (nth w1 (dec i))
                                    (nth w2 (dec j))) 0 1))))]))]
    (get (reduce f
                 {}
                 (for [x (range (inc (count w1)))
                       y (range (inc (count w2)))]
                   [x y]))
         [(count w1) (count w2)])))

(fn __ [s t]
  (if (= s t) 0
    (letfn
      [(f [s v0]
          (if (empty? s)
            (nth v0 (count t))
            (recur (rest s)
                   (loop [j 0
                          v1 (vec (list (inc (first v0))))]
                     (if (= j (count t))
                       v1
                       (recur (inc j)
                              (conj v1 (min (inc (nth v1 j))
                                            (inc (nth v0 (inc j)))
                                            (+ (nth v0 j)
                                               (if (= (first s) (nth t j))
                                                 0 1))))))))))]
      (f s (vec (range (inc (count t))))))))

(fn l
    ([w o]
       (let [wc (count w)
             oc (count o)]
         (cond
          (zero? oc) wc
          (zero? wc) oc
          :else
          (l 0 {-1 0} (vec w) (vec o)) )))
    ([n p w [c & r]] (pr n p)
       (if c
         (l (+ 1 n)
            (reduce #(conj % (min
                              (+   ({c 0} (get w %2 %2) 1)
                                   (get p (- %2 1) (max n %2)))
                              (+ 1 (get % (- %2 1) n))
                              (+ 1 (get p %2 %2))  ))
                    [] (range (count w))) w r)
         (last p))))

(fn [s1 s2]
  (let [cost (fn [c1 c2] (if (= c1 c2) 0 1))
        lev-it (fn [d cs i]
                 (reductions 
                   (fn [pc [[pd cd] c]] 
                     (if (zero? c) pd 
                       (+ c (min pc pd cd)))) 
                   i (map list (partition 2 1 d) cs)))]
    (last (reduce (fn [d c] 
                    (lev-it d (map (partial cost c) s1) 
                            (inc (first d)))) 
                  (range (inc (count s1))) s2))))

(fn levenstein [a b]
            (let [n (count a)
                  m (count b)
                  calc (fn [d [y x]]
                           (assoc-in d [y x]
                                     (if (= (nth a y)
                                            (nth b x))
                                         (get-in d [(dec y) (dec x)] (max x y))
                                         (->> [[-1 0] [-1 -1] [0 -1]]
                                              (map #(get-in d (vec (map + % [y x])) (max x y)))
                                              (apply min)
                                              (inc)))))]
                                              (if (or (empty? a) (empty? b))
                                                  (max (count a) (count b))
                                                  (->> (for [y (range n) x (range m)] [y x])
                                                       (reduce calc {})
                                                       (#(get-in % [(dec n) (dec m)] ))))))

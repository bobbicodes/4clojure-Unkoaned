(fn [a b]
  (last
    (reduce
      (fn [prevrow j]
        (reductions
          (fn [prev i]
            (min (inc prev) (inc (nth prevrow i))
                 (+ (nth prevrow (dec i)) 
                    (if (= (nth a (dec i)) (nth b (dec j))) 0 1))))
          j (range 1 (inc (count a)))))
      (range (inc (count a))) (range 1 (inc (count b))))))

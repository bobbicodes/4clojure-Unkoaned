(fn [s t]
  (let [s (vec s)
        t (vec t)
        m (count s)
        n (count t)
        d (reduce #(conj %1 (reduce conj [%2] (repeat m 0)))
                  [(-> m inc range)] (range 1 (inc n)))]
    (letfn [(gt [d i j] (-> d (nth j) (nth i)))
            (pt [d i j v] (assoc-in d [j i] v))]
      (-> (loop [j 1, d d]
            (if (> j n) d
                (recur (inc j)
                       (loop [i 1, d d]
                         (if (> i m) d
                             (recur (inc i)
                                    (let [i- (dec i) j- (dec j)]
                                      (if (= (s i-) (t j-))
                                        (pt d i j (gt d i- j-))
                                        (pt d i j
                                            (->> [(gt d i- j )
                                                  (gt d i  j-)
                                                  (gt d i- j-)]
                                              (map inc)
                                            (apply min)))))))))))
        last last))))

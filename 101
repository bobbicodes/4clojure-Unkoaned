(fn [s t] 
   ((fn f [j r] 
     (let [d (cons j r) 
           w (map #(list % %2 %3) s d r)
           z (if (< j (count t)) (reduce #(conj % (if (= (first %2) (nth t j)) (second %2) (min (inc (second %2)) (inc (last %2)) (inc (last %))))) [(inc j)] w) d)] 
       (if (= j (count t)) (last z) (f (inc j) (rest z))))) 0 (range 1 (inc (count s)))))

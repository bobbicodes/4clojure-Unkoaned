    (empty? s) (count t)
    (empty? t) (count s)
    :otherwise (let [cost (if (= (first s) (first t)) 0 1)]
                 (min (inc (lv lv (rest s) t)) (inc (lv lv s (rest t))) (+ cost (lv lv (rest s) (rest t)))))

(fn [s1 s2]
  (letfn [(calc-v [v0 c i]
              (loop [t s2
                     v1 [(inc i)]
                     v v0]
                (if (empty? t)
                  v1
                  (recur
                    (rest t)
                    (conj v1 (min (inc (last v1)) (inc (second v)) (+ (first v) (if (= c (first t)) 0 1))))
                    (rest v)))))]
    (cond
      (= s1 s2) 0
      (= s1 "") (count s2)
      (= s2 "") (count s1)
      :else (loop [v0 (range (inc (count s2)))
                   s s1
                   i 0]
              (if (empty? s)
                (last v0)
                (recur
                  (calc-v v0 (first s) i)
                  (rest s)
                  (inc i)))))))

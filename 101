(let [[c b l] [count butlast last]
      lev
      (fn [mem-lev s1 s2]
        (let [lev (fn [s1 s2] (mem-lev mem-lev s1 s2))]
          (cond (= s1 s2) 0
                (= (c s1) 0) (c s2)
                (= (c s2) 0) (c s1)
                true (min (+ (lev (b s1) s2) 1)
                          (+ (lev s1 (b s2)) 1)
                          (+ (if (= (l s1) (l s2)) 0 1) (lev (b s1) (b s2)))))))
      mem-lev (memoize lev)]
  #(mem-lev mem-lev % %2))

(fn lev [s t] 
  (let [lev* 
         (memoize (fn [lev* s t]
           (cond (zero? (count s)) (count t)
                 (zero? (count t)) (count s)
                 :else (min
                         (inc (lev* lev* (rest s) t))
                         (inc (lev* lev* s (rest t)))
                         (+ (lev* lev* (rest s) (rest t))
                            (if (= (first s) (first t)) 0 1))))))]
  (lev* lev* s t)))

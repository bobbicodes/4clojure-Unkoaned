(fn levd [s t]
   (let [cs (count s) ct (count t)
         sr (range (inc cs)) tr (range (inc ct))
         si (zipmap (map (fn [x] [x 0]) sr) sr)
         ti (zipmap (map (fn [y] [0 y]) tr) tr)
         dm (into si ti)]
     (loop [m dm i 1 j 1]
       (if (m [cs ct]) (m [cs ct])
         (recur (conj 
                 {[i j]
                  (if (= (nth s (dec i)) (nth t (dec j))) (m [(dec i) (dec j)])
                    (inc (min (m [(dec i) j])
                              (m [i (dec j)])
                              (m [(dec i) (dec j)])
                              )))
                  } m)
                (if (= i cs) 1 (inc i))
                (if (= i cs) (inc j) j))))))

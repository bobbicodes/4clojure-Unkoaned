(letfn [(distance [step xs ys]
          (cond
           (empty? xs)
           (count ys)
           
           (empty? ys)
           (count xs)
           
           :else
           (let [[x & sub-xs] xs
                 [y & sub-ys] ys
                 cost (if (= x y) 0 1)]
             (min (inc (step step sub-xs ys))
                  (inc (step step xs sub-ys))
                  (+ cost (step step
                                sub-xs
                                sub-ys))))))]
  
  (let [step (memoize distance)]
    (fn levenshtein [xs ys]
      (step step xs ys))))

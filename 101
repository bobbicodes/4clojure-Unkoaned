(fn [C s t]
  ((fn f [i j]
     (cond 
       (= i 0) j
       (= j 0) i
       (= (nth s (- i 1)) (nth t (- j 1))) (f (- i 1) (- j 1))
       :else   (+ 1 
                  (min (f (- i 1) j)
                       (f i       (- j 1))
                       (f (- i 1) (- j 1))))))
   (C s) (C t))) count

(fn q [str1 str2]
  (let [p (atom false)]
    (reset! p (memoize (fn [i1 i2 len1 len2]
                         (cond (= len1 i1) (- len2 i2)
                               (= len2 i2) (- len1 i1)
                               :else
                               (min (inc (@p (inc i1) i2 len1 len2))
                                    (inc (@p i1 (inc i2) len1 len2))
                                    (+ (if (= (nth str1 i1) (nth str2 i2)) 0 1)
                                       (@p (inc i1) (inc i2) len1 len2)))))))
    (@p 0 0 (count str1) (count str2))))

(fn df [a b]
    (let [alen (count a) blen (count b)]
      (cond (= alen blen) (count (filter true? (map not= a b)))
            (> alen blen) (df b a)
            ; from here alen < blen
            (= blen 0) 0
            :else (min 
                    (+ 1 (df a (rest b))) 
                    (+ (if (= (first a) (first b)) 0 1) (df (rest a) (rest b)))))
      ))

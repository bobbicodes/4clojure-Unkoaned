; This would've been much simpler if I could actually use memoize.
(fn lev [s t]
  (cond
   (= s t) 0
   (empty? s) (count t)
   (empty? t) (count s)
   :else
   (loop [v0 (range (inc (count t)))
          i 0]
     (if (= i (count s))
       (last v0)
       (recur
        (loop [v1 [(inc i)]
               j 0]
          (if (= j (count t))
            v1
            (recur
             (conj v1
                   (min (inc (last v1))
                        (inc (nth v0 (inc j)))
                        (+ (nth v0 j) (if (= (nth s i) (nth t j)) 0 1))))
             (inc j)
             )
            )
          )
        (inc i)
        )
       )
     )
   )
  )

(fn [s t]
  (let [dist
        (fn [s t mdist]
          (let [sl (count s)
                tl (count t)]
            (cond
             (zero? sl) tl
             (zero? tl) sl
             :else (let [si (dec sl)
                         ti (dec tl)
                         cost (if (= (last s) (last t)) 0 1)
                         s1 (take si s)
                         t1 (take ti t)]
                     (min
                      (inc (mdist s1 t mdist))
                      (inc (mdist s t1 mdist))
                      (+ (mdist s1 t1 mdist) cost))))))]
    (dist s t (memoize dist))))

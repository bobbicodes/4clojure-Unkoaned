(fn levenshtein [s1 s2]
  (let [f (fn [rec a b]
            (let [[ca cb] (map count [a b])]
              (if (some zero? [ca cb]) (max ca cb)
                (min (inc (rec rec (pop a) b))
                     (inc (rec rec a (pop b)))
                     (+ (rec rec (pop a) (pop b))
                        (if (= (peek a) (peek b)) 0 1))))))]
    (f (memoize f) (vec s1) (vec s2))))

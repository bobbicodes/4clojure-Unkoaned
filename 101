(let [f (fn [f [af & ar :as a] [bf & br :as b]]
            (if (and (seq a) (seq b))
              (if (= af bf)
                (f f ar br)
                (inc (min (f f a br) (f f ar b) (f f ar br))))
              (+ (count a) (count b))))]
    (fn [a b] (f (memoize f) a b)))

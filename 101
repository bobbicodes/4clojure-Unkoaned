  (reductions
    (fn [acc i]
      (if (= (nth %2 j) (nth % i)) (nth r i)
        (inc (min acc (nth r (inc i)) (nth r i)))))
    (inc j)
    (range (count %))))
  (range (inc (count %)))
  (range (count %2))))

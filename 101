(fn [x y]
  (let [u (vec x)
        v (vec y)
        M (count u)
        N (count v)
        R (fn [P m]
            (loop [D [m] n 1]
              (if (< N n)
                D
                (let [dm (dec m)
                      dn (dec n)
                      c (if (= (u dm) (v dn)) 0 1)
                      v (min (inc (P n))
                             (inc (last D))
                             (+ (P dn) c))]
                  (recur (conj D v) (inc n))))))]
    (loop [D (vec (range (inc N))) m 1]
      (if (< M m) (last D) (recur (R D m) (inc m))))))

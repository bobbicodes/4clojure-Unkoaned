(fn prob101
  [a b]
  (let [levenshtein (fn [fnc str1 str2]
                     (let [len1 (count str1)
                           len2 (count str2)]
                       (cond (zero? len1) len2
                             (zero? len2) len1
                             :else
                             (let [cost (if (= (first str1) (first str2)) 0 1)]
                               (min (inc (fnc fnc (rest str1) str2))
                                    (inc (fnc fnc  str1 (rest str2)))
                                    (+ cost
                                       (fnc fnc (rest str1) (rest str2))))))))]
    (levenshtein (memoize levenshtein) a b)))

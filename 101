(fn [s t]
  (let [sl (count s)
        tl (count t)]
    (cond
     (= s t) 0
     (empty? s) tl
     (empty? t) sl
     :else
     (last
      (loop [i 0
             v (-> tl inc range vec)]
        (if (= i sl) v
            (recur (inc i)
                   (loop [v-new [(inc i)]]
                     (if (= tl (-> v-new count dec)) v-new
                         (let [j (-> v-new count dec)
                               cost (if (= (nth s i) (nth t j)) 0 1)
                               v0_j (nth v j)
                               v0_jp1 (nth v (inc j))
                               v1_j (last v-new)]
                           (recur
                            (conj v-new
                                  (min
                                   (inc v1_j)
                                   (inc v0_jp1)
                                   (+ v0_j cost))))))))))))))

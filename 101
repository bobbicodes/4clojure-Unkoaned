(fn lev [a b]
  (letfn [(lev-line [lastr ch s]
            (loop [ [a b & r] lastr
                    result     [(inc (first lastr))]
                    [s0 & sr]  s]
              (if s0
                (recur
                  (conj r b)
                  (conj result (apply min [(inc b) (inc (last result)) (if (= s0 ch) a (inc a))]))
                  sr)
                result)))]
    (if (= a b)
      0
      (last (reduce
        (fn [r ch] (lev-line r ch a))
        (range (+ 2 (count a)))
        b)))))

(fn [s t]
  (if (= s t)
    0
    (if (zero? (count s))
      (count t)
      (if (zero? (count t))
        (count s)
        (loop [i 0 v0 (apply vector (range 0 (inc(count t))))]
          (if (= i (count s))
            (last v0)               
             (recur
              (inc i)
              (loop [j 0 v1 (apply vector (inc i) (repeat (count t) 0))]
                (if (= j (count t))
                   v1
                   (recur
                    (inc j)
                    (update-in v1 [(inc j)]
                               (fn [_](min
                                 (inc(nth v1 j))
                                 (inc(nth v0 (inc j)))
                                 (+ (nth v0 j) (if (= (nth s i) (nth t j)) 0 1))  )))))))))))))

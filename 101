(fn [s t]
  (let [[sc tc] (map count [s t])]
    (or ({s 0} t)
        ({sc tc} 0 ({tc sc} 0))
        (#(get (reduce % `[~@(range (inc tc))] (range sc)) tc)
         #(reduce (fn [x y] (conj x
             (min (inc (get x y))
                  (inc (get %1 (inc y)))
                  (+ (get %1 y) ({(get s %2) 0} (get t y) 1))) ))
           [(inc %2)] (range tc))) )))

(fn [s1 s2]
  (let [d (memoize (fn [i j dst]
    (cond
      (= 0 i) j
      (= 0 j) i
      :else (if (= (get s1 (- i 1)) (get s2 (- j 1)))
        (dst (- i 1) (- j 1) dst)
        (+ 1 (reduce min 
          [(dst (- i 1) j dst)
           (dst i (- j 1) dst)
           (dst (- i 1) (- j 1) dst)]))))))]
    (d (count s1) (count s2) d)))

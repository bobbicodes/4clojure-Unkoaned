(fn edit-dist [a b]
  (cond (not b) (count a) (not a) (count b) 
        :else (let [ra (next a) rb (next b)]
(if (= (first a) (first b)) (edit-dist ra rb) 
  (+ 1 (min (edit-dist ra rb) (edit-dist ra b) (edit-dist a rb)))))))

(fn [s t]
  (let [distance
        (memoize (fn [f s t]
                   (if (empty? s) (count t)
                     (if (empty? t) (count s)
                       (min (inc (f f s (butlast t)))
                            (inc (f f t (butlast s)))
                            (+ (if (= (last s) (last t)) 0 1)
                               (f f (butlast s) (butlast t))))))))]
    (distance distance s t)))

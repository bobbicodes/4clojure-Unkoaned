(fn levenstein [a b]
  (let [lev (fn lev [a b]
            (cond (= a b) 0
                  (= a []) (count b)
                  (= b []) (count a)
                  :else (min (if (< (count a) (count b)) (inc (lev a (rest b)))
                                (inc (lev (rest a) b)))
                             (+ (lev (rest a) (rest b)) (if (= (first a) (first b)) 0 1)))))
        mem-lev (memoize lev)]
    (mem-lev a b)))

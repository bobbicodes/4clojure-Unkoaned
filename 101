(fn leven [x y] 
  (let [l (memoize (fn [f x y]
    (cond (empty? x) (count y)
          (empty? y) (count x)
           :else (min (+ (f f (rest x) y) 1)
                      (+ (f f x (rest y)) 1)
                      (+ (f f (rest x) (rest y)) (if (= (first x) (first y)) 0 1))
                  )
    )
    ))
    magic (partial l l)]
    (magic x y)
    )
)

(memoize (fn lev [xs ys]
  (cond
    (= (count xs) 0) (count ys)
    (= (count ys) 0) (count xs)
    (= (first xs) (first ys)) (lev (rest xs) (rest ys))
    :else (+ 1 (min (lev (rest xs) (rest ys)) (lev xs (rest ys)) (lev (rest xs) ys))))))

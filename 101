(fn levenshtein [a b]
  (cond (empty? a) (count b)
        (empty? b) (count a)
        (= (first a) (first b)) (levenshtein (rest a) (rest b))
        (= (rest a) (rest b)) 1
        (= (seq a) (rest b)) 1
        (= (rest a) (seq b)) 1
        :else (inc (min (levenshtein (rest a) (rest b))
                        (levenshtein (seq a) (rest b))
                        (levenshtein (rest a) (seq b))))))

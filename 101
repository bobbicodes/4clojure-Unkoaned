(fn lev-dist--compat [x y]
  (let [lev-dist--memo
        (fn [a b f]
          (cond
            (empty? a)     (count b)
            (empty? b)     (count a)
            (= (first a)
               (first b))  (f (rest a) (rest b) f)
            :else          (inc (min (f a (rest b) f)
                                     (f (rest a) b f)
                                     (f (rest a) (rest b) f)))))]
    (lev-dist--memo x y (memoize lev-dist--memo))))

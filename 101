(fn [xs ys]
   ((fn calc-next [xs ys rs]
      (let [y (first ys)]
        (if (nil? y)
          (last rs)
          (calc-next xs (rest ys)
                     ;; loop to get next rs
                     (loop [xs xs
                            rs rs
                            nrs [(inc (first rs))]]
                       (if (empty? xs) nrs
                           (recur (rest xs)
                                  (rest rs)
                                  (conj nrs (min (inc (last nrs))
                                                 (inc (second rs))
                                                 (+ (first rs)
                                                    (if (= (first xs) y) 0 1)))))
                           )
                       ))
          )
 
        )
      ) xs ys (range (inc (count xs)))))

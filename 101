(fn [a b]
    (let [f (fn [g i j] (cond
                         (= 0 i) j
                         (= 0 j) i
                         :else (min (inc (g g (dec i) j))
                                    (inc (g g i (dec j)))
                                    (+ (g g (dec i) (dec j))
                                       (if (= (nth a (dec i)) (nth b (dec j)))
                                         0
                                         1)))))
          g (memoize f)]
      (g g (count a) (count b))))

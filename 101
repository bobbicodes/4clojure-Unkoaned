(fn
  [s1 s2]
  (let [lev (memoize
              (fn [y i j]
                (if (= 0 (min i j))
                  (max i j)
                  (let [cost (if (= (get s1 (dec i))
                                    (get s2 (dec j)))
                               0
                               1)]
                    (min (inc (y y (dec i) j))
                         (inc (y y i (dec j)))
                         (+ cost (y y (dec i) (dec j))))))))]
    (lev lev (count s1) (count s2))))

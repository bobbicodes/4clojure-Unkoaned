(fn [x y]
  (cond
   (= x y) 0
   (> (count x) (count y)) (recur y x)
   (= 0 (compare (first x) (first y))) (recur (rest x) (rest y))
   (= 0 (compare (last x) (last y))) (recur (butlast x) (butlast y))
   :else (count (filter false?
                        (map = (concat x (repeat (- (count y) (count x)) \*)) y)))
   ))

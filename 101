(fn distance [a b]
  (letfn [(walk
           [f a b]
           (cond
             (or (nil? a) (nil? b))
             (+ (count a) (count b))
             
             (= (first a) (first b))
             (f f (next a) (next b))
            
             :else
             (inc (min (f f (next a) (next b))
                       (f f (next a) b)
                       (f f a (next b))))))]
    (walk (memoize walk) a b)))

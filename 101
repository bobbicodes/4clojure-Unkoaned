(fn lev [[h & t :as a] [f & r :as b]]
  (cond (nil? h) (count b)
        (nil? f) (count a)
        (= f h)  (recur t r)
        :else (min (inc (lev t r))
                   (inc (lev a r))
                   (inc (lev t b)))
        ))

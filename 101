(fn levenshtein-distance [s1 s2]                                                                                                                        
    (->> (for [l1 s1 l2 s2] [l1 l2])                                                                                            
         (partition (count s2))                                                                                                                         
         (reduce                                                                                                                
            (fn [prev row]                                                                                                                              
                ; build the next row of distances                                                                                                       
                (reduce                                                                                                         
                    (fn [cur [a b]]                                                                                                                     
                        (let [i (count cur)]                                                                                    
                            (conj cur                                                                                           
                                (if (= a b) (get prev (dec i))                                                                                          
                                            (+ (min (get prev i) (last cur) (get prev (dec i))) 1)))))                          
                    [(inc (first prev))]                                                                                                                
                    row))                                                                                                       
            (vec (range (inc (count s2)))))                                                                                                             
         (last)))

(fn [a b]
  ((reduce (fn [acc [i j]]
                      (assoc acc [i j]
                             (if (zero? (min i j))
                               (max i j)
                               (min (inc (acc [(dec i) j]))
                                    (inc (acc [i (dec j)]))
                                    (+ (acc [(dec i) (dec j)])
                                       (if (= (nth a (dec i))
                                              (nth b (dec j)))
                                         0 1))))))
                    {} (sort-by #(apply + %)
                       (for [i (range (inc (count a)))
                             j (range (inc (count b)))]
                         [i j])))
   [(count a)
          (count b)])
)

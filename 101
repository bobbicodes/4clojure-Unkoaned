(fn [& i]
  (let [c count
        [x y] (sort #(< (c %1) (c %2)) i)]
    (loop [s x l y n (c y)]
      (if s
        (let [a (first s)
              f ((set l) a)]
            (recur
                (next s)
                (if f (rest (drop-while #(not= a %) l)) l)
                (if f (dec n) n)))
        n))))

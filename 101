(fn [x y]
  (letfn
      [(levenshtein [mem s1 s2]
         (let [l1 (count s1)
               l2 (count s2)]
           (cond (zero? l1) l2
                 (zero? l2) l1
                 :else
                 (let [cost (if (= (first s1) (first s2)) 0 1)]
                   (min (inc (mem mem (rest s1) s2))
                        (inc (mem mem s1 (rest s2)))
                        (+ cost
                           (mem mem (rest s1) (rest s2))))))))]
    (levenshtein (memoize levenshtein) x y)))

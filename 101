(fn f [a b]                    
  (cond (empty? a) (count b)
        (empty? b) (count a)
        (= (first a) (first b)) (recur (rest a) (rest b))
        :else (inc (min (f (rest a) b) 
                        (f a (rest b)) 
                        (f (rest a) (rest b))))))

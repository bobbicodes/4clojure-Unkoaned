(fn f [s t]
  (cond (= s t) 0
        (empty? s) (count t)
        (empty? t) (count s)
        :else
        (loop [i 0 v (range 0 (inc (count t)))]
          (if (= i (count s))
            (last v)
            (recur (inc i)
                   (loop [j 0 v1 [(inc i)]]
                     (if (= j (count t))
                       v1
                       (let [cost (if (= (nth s i) (nth t j)) 0 1)]
                         (recur (inc j) (concat v1 [(min (inc (last v1)) (inc (nth v (inc j))) (+ (nth v j) cost))]))))))))))

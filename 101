(letfn [(implicit-dp [rec sa sb]
          (cond (empty? sa) (count sb)
                (empty? sb) (count sa)
                :else (min
                        (+ (if (= (first sa) (first sb))
                             0
                             1)
                           (rec rec (rest sa) (rest sb)))
                        (+ 1 (rec rec sa (rest sb)))
                        (+ 1 (rec rec (rest sa) sb)))))
        (leven-dp [sa sb]
          (let [dp (memoize implicit-dp)]
            (dp dp sa sb)))]
  leven-dp)

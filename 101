(fn [s t]
  (let [f (fn [f s t]
            (let [sc (count s)
                  tc (count t)]
              (cond
                (= sc 0) tc
                (= tc 0) sc
                :else (let [cost (if (= (last s) (last t)) 0 1)]
                        (min (inc (f f (butlast s) t))
                             (inc (f f s (butlast t)))
                             (+ (f f (butlast s) (butlast t)) cost))))))
        mf (memoize f)]
    (mf mf s t)))

(fn [s d]
  (loop [a [[0 s d]]]
    (let [m (keep (fn [[l x y]] (if (= x y) l)) a)]
      (if (not (empty? m))
          (apply min m)
          (recur
            (mapcat
              (fn [[l [sf & sr :as s] [df & dr :as d]]]
                (if (= sf df)
                    (vector [l sr dr])
                    (vector [(inc l) s dr] [(inc l) sr d] [(inc l) sr dr])))
              a))))))

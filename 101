(fn [w1 w2]
  (cond
    (= w1 w2) 0
    (empty? w1) (count w2)
    (empty? w2) (count w1)
    :else
      (letfn [(get-reduce-fn [[ltr _]]
                             (fn [[l v] [[l0 v0] [l1 v1]]]
                               (list l1
                                 (min (inc v1) (inc v)
                                   (+ v0 (if (= ltr l1) 0 1))))))
              (next-row [row first-item]
                        (partition 2 1
                                   (reductions
                                     (get-reduce-fn first-item)
                                     first-item
                                     row)))
              (make-row [w] (map list w (iterate inc 1)))
              (make-first-row [w] (cons '(nil 0) (make-row w)))]
        (->> (make-first-row w2)
             (reduce 
               next-row
               (partition 2 1 (make-first-row w1)))
             ((comp last last last))))))

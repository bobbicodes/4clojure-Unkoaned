(fn [s t]
  (let [sn (count s) tn (count t)]
    (cond
      (= s t) 0
      (zero? sn) tn
      (zero? tn) sn
      :else
        (->>
          (reduce
            (fn [[i v0] s-item]
              [
                (inc i)
                (reductions 
                  #(min (inc %) %2)
                  (inc i)
                  (map min (map inc (drop 1 v0))
                    (map + v0 (map #(if (= s-item %) 0 1) t))
                  )
                )
              ]
            )
            [0 (range (inc tn))]
            s
          )
          second
          last
) ) )   )

(letfn [(lev [lf s1 s2]
                (cond (empty? s1) (count s2)
                      (empty? s2) (count s1)
                      :otherwise (min (inc (lf lf (rest s1) s2))
                                      (inc (lf lf s1 (rest s2)))
                                      (+ (lf lf (rest s1) (rest s2)) (if (= (first s1) (first s2))
                                                                     0
                                                                     1)))))]
        (let [mem (memoize lev)]
          (partial mem mem)))

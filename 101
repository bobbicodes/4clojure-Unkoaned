(fn distance [xs ys]
  (cond
    (empty? xs) (count ys)
    (empty? ys) (count xs)
    true
    (let [[x & xs'] xs
          [y & ys'] ys]
      (if (= x y)
        (distance xs' ys')
        (inc (min (distance xs' ys') (distance xs' ys) (distance xs ys')))))))
